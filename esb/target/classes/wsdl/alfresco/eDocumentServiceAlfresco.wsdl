<?xml version='1.0' encoding='UTF-8'?>
<definitions
    xmlns:wssutil="http://docs.oasis-open.org/wss/2004/01/oasis-200401-wss-wssecurity-utility-1.0.xsd"
    xmlns:wsp="http://schemas.xmlsoap.org/ws/2004/09/policy"
    xmlns:soap="http://schemas.xmlsoap.org/wsdl/soap/"
    xmlns:tns="http://cmp.vba.va.gov/external/eDocumentServiceAlfresco/"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema"
    xmlns="http://schemas.xmlsoap.org/wsdl/"
    targetNamespace="http://cmp.vba.va.gov/external/eDocumentServiceAlfresco/" name="eDocumentServiceAlfresco">
