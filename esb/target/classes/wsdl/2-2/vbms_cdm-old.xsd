<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns="http://vbms.vba.va.gov/cdm"
           targetNamespace="http://vbms.vba.va.gov/cdm" elementFormDefault="qualified">
    <xs:complexType name="Address">
        <xs:sequence>
            <xs:element name="source" type="Source" minOccurs="1"/>
            <xs:element name="createDt" type="xs:date" minOccurs="0"/>
            <xs:element name="lastUpdateDt" type="xs:date" minOccurs="0"/>
        </xs:sequence>
        <xs:attribute name="ID" type="xs:long" use="required"/>
        <xs:attribute name="addressLine1" type="xs:string" use="required"/>
        <xs:attribute name="addressLine2" type="xs:string" use="optional"/>
        <xs:attribute name="addressLine3" type="xs:string" use="optional"/>
        <xs:attribute name="AddressType_ID" type="xs:long"/>
        <xs:attribute name="city" type="xs:string" use="required"/>
        <xs:attribute name="State_Code" type="xs:string"/>
        <xs:attribute name="Country_Code" type="xs:string"/>
        <xs:attribute name="postalCode" type="xs:string" use="required"/>
        <xs:attribute name="preferredAddrInd" type="xs:boolean" use="required"/>
        <xs:attribute name="jpaVersion" type="xs:long" use="required"/>
    </xs:complexType>
    <xs:complexType name="AlternateName">
        <xs:sequence>
            <xs:element name="source" type="Source" minOccurs="0"/>
            <xs:element name="namePrefix" type="NamePrefix" minOccurs="0"/>
            <xs:element name="createDt" type="xs:date" minOccurs="0"/>
            <xs:element name="lastUpdateDt" type="xs:date" minOccurs="0"/>
        </xs:sequence>
        <xs:attribute name="ID" type="xs:long" use="required"/>
        <xs:attribute name="Person_ID" type="xs:long" use="required"/>
        <xs:attribute name="firstName" type="xs:string" use="required"/>
        <xs:attribute name="middleName" type="xs:string"/>
        <xs:attribute name="lastName" type="xs:string" use="required"/>
        <xs:attribute name="NameSuffix_ID" type="xs:long"/>
    </xs:complexType>
    <xs:complexType name="ContentionAnalysisResult">
        <xs:sequence>
            <xs:element name="bodySystems" type="BodySystem" maxOccurs="unbounded" minOccurs="0"/>
            <xs:element name="dbqs" type="DBQView" maxOccurs="unbounded" minOccurs="0"/>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="Annotation">
        <xs:sequence>
            <xs:element name="content" type="xs:string"/>
            <xs:element name="createDt" type="xs:date"/>
            <xs:element name="lastUpdateDt" type="xs:date" minOccurs="0"/>
        </xs:sequence>
        <xs:attribute name="ID" type="xs:long" use="required"/>
        <xs:attribute name="documentID" type="xs:string" use="required"/>
        <xs:attribute name="text" type="xs:string"/>
        <xs:attribute name="type" type="xs:string" use="required"/>
        <xs:attribute name="createUser_ID" type="xs:long" use="required"/>
        <xs:attribute name="modifiedUser_ID" type="xs:long"/>
        <xs:attribute name="jpaVersion" type="xs:long" use="required"/>
    </xs:complexType>
    <xs:complexType name="Award">
        <xs:sequence>
            <xs:element name="awardStatusDt" type="xs:date"/>
            <xs:element name="lastPaidDt" type="xs:date" minOccurs="0"/>
            <xs:element name="retroactiveDt" type="xs:date" minOccurs="0"/>
            <xs:element name="createDt" type="xs:date" minOccurs="0"/>
            <xs:element name="lastUpdateDt" type="xs:date" minOccurs="0"/>
        </xs:sequence>
        <xs:attribute name="ID" type="xs:long" use="required"/>
        <xs:attribute name="Issue_ID" type="xs:long" use="required"/>
        <xs:attribute name="Veteran_Person_ID" type="xs:long" use="required"/>
        <xs:attribute name="beneficiaryPerson_ID" type="xs:long" use="required"/>
        <xs:attribute name="benefitParticipant_Person_ID" type="xs:long" use="required"/>
        <xs:attribute name="receiptParticipant_Person_ID" type="xs:long" use="required"/>
        <xs:attribute name="AwardBenefit_ID" type="xs:string"/>
        <xs:attribute name="AwardPayStatus_ID" type="xs:string"/>
        <xs:attribute name="AwardStatus_ID" type="xs:string"/>
        <xs:attribute name="accountBalanceAmt" use="required">
            <xs:simpleType>
                <xs:restriction base="xs:decimal">
                    <xs:fractionDigits value="2"/>
                </xs:restriction>
            </xs:simpleType>
        </xs:attribute>
        <xs:attribute name="beneficiaryType" type="xs:string" use="required"/>
        <xs:attribute name="deductionsAmt">
            <xs:simpleType>
                <xs:restriction base="xs:decimal">
                    <xs:fractionDigits value="2"/>
                </xs:restriction>
            </xs:simpleType>
        </xs:attribute>
        <xs:attribute name="fiduciaryType" type="xs:string"/>
        <xs:attribute name="Frequency_ID" type="xs:string"/>
        <xs:attribute name="igReferenceNbr" type="xs:string"/>
        <xs:attribute name="requestedFrequency" type="xs:string"/>
        <xs:attribute name="Document_ID" type="xs:long" use="required"/>
    </xs:complexType>
    <xs:complexType name="BenefitType">
        <xs:sequence>
            <xs:element minOccurs="0" name="createDt" type="xs:date"/>
            <xs:element minOccurs="0" name="lastUpdateDt" type="xs:date"/>
            <xs:element minOccurs="0" name="expirationDt" type="xs:date"/>
        </xs:sequence>
        <xs:attribute name="ID" type="xs:long" use="required"/>
        <xs:attribute name="code" type="xs:string" use="required"/>
        <xs:attribute name="name" type="xs:string" use="required"/>
        <xs:attribute name="description" type="xs:string"/>
        <xs:attribute name="jpaVersion" type="xs:long" use="required"/>
    </xs:complexType>
    <xs:complexType name="BodySystem">
        <xs:sequence>
            <xs:element minOccurs="1" name="createDt" type="xs:date"/>
            <xs:element minOccurs="1" name="lastUpdateDt" type="xs:date"/>
            <xs:element minOccurs="0" name="expirationDt" type="xs:date" />
        </xs:sequence>
        <xs:attribute name="suggestedInd" type="xs:boolean" use="required"/>
        <xs:attribute name="ID" type="xs:long" use="required"/>
        <xs:attribute name="mainSystem" type="xs:string" use="required"/>
        <xs:attribute name="subSystem" type="xs:string" use="required"/>
    </xs:complexType>
    <xs:complexType name="BodySystemHistorical">
        <xs:sequence>
            <xs:element name="expirationDt" type="xs:date" minOccurs="0"/>
            <xs:element name="createDt" type="xs:date" minOccurs="0"/>
            <xs:element name="lastUpdateDt" type="xs:date" minOccurs="0"/>
            <xs:element name="bodySystem" type="BodySystem"/>
        </xs:sequence>
        <xs:attribute name="ID" type="xs:long" use="required"/>
        <xs:attribute name="contentionText" type="xs:string" use="required"/>
    </xs:complexType>
    <xs:complexType name="Exam">
        <xs:sequence>
            <xs:element name="createDt" type="xs:date"/>
            <xs:element name="lastUpdateDt" type="xs:date" minOccurs="0"/>
            <xs:element name="expirationDt" type="xs:date" minOccurs="0"/>
        </xs:sequence>
        <xs:attribute name="ID" type="xs:long" use="required"/>
        <xs:attribute name="name" type="xs:string" use="required"/>
    </xs:complexType>
    <xs:complexType name="ExamDbqAssoc">
        <xs:sequence>
            <xs:element name="createDt" type="xs:date"/>
            <xs:element name="lastUpdateDt" type="xs:date" minOccurs="0"/>
            <xs:element name="expirationDt" type="xs:date" minOccurs="0"/>
            <xs:element name="dbq" type="DBQ"/>
            <xs:element name="exam" type="Exam"/>
        </xs:sequence>
        <xs:attribute name="ID" type="xs:long" use="required"/>
    </xs:complexType>
    <xs:complexType name="ClaimTypeEndProductInput">
        <xs:attribute name="benefitTypeId" type="xs:string"/>
        <xs:attribute name="payeeCode" type="xs:string" use="required"/>
        <xs:attribute name="benefitTypeCd" type="xs:string" use="required"/>
        <xs:attribute name="shareCommandTypeCode" type="xs:string" use="required"/>
        <xs:attribute name="serviceTypeCode" type="xs:string" use="required"/>
    </xs:complexType>
    <xs:complexType name="Claim">
        <xs:sequence>
            <xs:element name="EndProductClaimType" type="CodeValue">
                <xs:annotation>
                    <xs:documentation>List of VETSNET ClaimTypeCode,ClaimTypeName pairs</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="claimDateDt" type="xs:date"/>
            <xs:element name="flashList" type="Flash" maxOccurs="unbounded" minOccurs="0"/>
            <xs:element name="suspenseDate" type="xs:date" minOccurs="0"/>
            <xs:element name="contentions" type="Contention" maxOccurs="unbounded"/>
            <xs:element name="inTakeSite" type="xs:string" minOccurs="0" />
            <xs:element name="suppressAckLetterInd" type="xs:boolean" default="false" />
            <xs:element name="futureReason" type="xs:string" minOccurs="0" />
        </xs:sequence>
        <xs:attribute name="ID" type="xs:long" use="required"/>
        <xs:attribute name="Participant_Person_ID" type="xs:long" use="required"/>
        <xs:attribute name="PowerOfAttorney_ID" type="xs:string"/>
        <xs:attribute name="Veteran_Person_ID" type="xs:long" use="required"/>
        <xs:attribute name="InsuranceInfo_ID" type="xs:long"/>
        <xs:attribute name="benefitTypeCd" type="xs:string">
            <xs:annotation>
                <xs:documentation>Maps to BenefitType - Live, Death, CH31</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="claimLevelStatusCd" type="xs:string" use="required"/>
        <xs:attribute name="modifiedEndProductCd" type="xs:string"/>
        <xs:attribute name="gulfWarRegistryInd" type="xs:boolean" use="required"/>
        <xs:attribute name="label" type="xs:string" use="required"/>
        <xs:attribute name="payeeCd" type="xs:string" use="required"/>
        <xs:attribute name="sectionUnit" type="xs:string"/>
        <xs:attribute name="serviceTypeCd" type="xs:string"/>
        <xs:attribute name="specialIssueCd" type="xs:string"/>
        <xs:attribute name="stationOfJurisdiction" type="xs:string" use="required"/>
        <xs:attribute name="suspenseReasonCd" type="xs:string"/>
        <xs:attribute name="priority" type="xs:string" use="required"/>
        <xs:attribute name="td" type="xs:string"/>
        <xs:attribute name="disposition" type="xs:string"/>
        <xs:attribute name="folderWithClaim" type="xs:string"/>
        <xs:attribute name="preDischargeInd" type="xs:boolean" default="false" />
    </xs:complexType>
    <xs:complexType name="ClaimWorkItem">
        <xs:complexContent>
            <xs:extension base="WorkItem">
                <xs:sequence>
                    <xs:element name="EndProductClaimType" type="CodeValue">
                        <xs:annotation>
                            <xs:documentation>List of VETSNET ClaimTypeCode,ClaimTypeName pairs</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="suspenseDate" type="xs:date" minOccurs="0"/>
                    <xs:element name="claimDateDt" type="xs:date" minOccurs="0"/>
                    <xs:element name="claimStatus" type="ClaimStatus"/>
                </xs:sequence>
                <xs:attribute name="claimID" type="xs:long" use="required"/>
                <xs:attribute name="modifiedEndProductCd" type="xs:long"/>
                <xs:attribute name="claimStatusName" type="xs:string"/>
                <xs:attribute name="claimStatusCode" type="xs:string"/>
                <xs:attribute name="participantPersonId" type="xs:long"/>
                <xs:attribute name="stationOfJurisdiction" type="xs:string"/>
                <xs:attribute name="veteranPersonID" type="xs:long"/>
                <xs:attribute name="terminalDigit" type="xs:long"/>
                <xs:attribute name="contentionsCount" type="xs:long"/>
                <xs:attribute name="claimantFirstName" type="xs:string"/>
                <xs:attribute name="claimantLastName" type="xs:string"/>
                <xs:attribute name="veteranFirstName" type="xs:string"/>
                <xs:attribute name="veteranLastName" type="xs:string"/>
                <xs:attribute name="fileNumber" type="xs:string" />
                <xs:attribute name="vsrId" type="xs:long" />
                <xs:attribute name="suspenseReason" type="xs:string" />
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="BrokeredClaimWorkItem">
        <xs:complexContent>
            <xs:extension base="ClaimWorkItem">
                <xs:sequence>
                    <xs:element name="brokeredItem" type="BrokeredItem"/>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="CodeValue">
        <xs:sequence>
            <xs:element name="createDt" type="xs:date"/>
            <xs:element name="lastUpdateDt" type="xs:date" minOccurs="0"/>
            <xs:element name="expirationDt" type="xs:date" minOccurs="0"/>
        </xs:sequence>
        <xs:attribute name="code" type="xs:string" use="required"/>
        <xs:attribute name="name" type="xs:string" use="required"/>

    </xs:complexType>
    <xs:complexType name="Contention">
        <xs:sequence>
            <xs:element name="createDt" type="xs:date" minOccurs="0"/>
            <xs:element name="completionDate" type="xs:date" minOccurs="0"/>
            <xs:element name="source" type="Source" minOccurs="0"/>
            <xs:element name="submitDate" type="xs:date"/>
            <xs:element name="issue" minOccurs="0" type="Issue" maxOccurs="unbounded">
                <xs:annotation>
                    <xs:documentation>Associations between Contentions and Issue are maintained by the
                        ISSUE_CONTENTION_ASSOC table in the PDM.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="startDate" type="xs:date"/>
        </xs:sequence>
        <xs:attribute name="ID" type="xs:string" use="required"/>
        <xs:attribute name="Claim_ID" type="xs:long" use="required"/>
        <xs:attribute name="actionableItemInd" type="xs:boolean" use="required"/>
        <xs:attribute name="awaitingResponse" type="xs:string" use="required"/>
        <xs:attribute name="classificationCd" type="xs:string"/>
        <xs:attribute name="decisionHistoryCd" type="xs:string"/>
        <xs:attribute name="decisionHistoryNotes" type="xs:string"/>
        <xs:attribute name="decisionStatusCode" type="xs:string"/>
        <xs:attribute name="fileNumber" type="xs:string"/>
        <xs:attribute name="levelStatusCode" type="xs:string"/>
        <xs:attribute name="medicalInd" type="xs:boolean" use="required"/>
        <xs:attribute name="partcipantContention" type="xs:string" use="required"/>
        <xs:attribute name="secondaryToContentionID" type="xs:string" use="required"/>
        <xs:attribute name="title" type="xs:string" use="required"/>
        <xs:attribute name="typeCode" type="xs:string"/>
        <xs:attribute name="workingContention" type="xs:string" use="required"/>
        <xs:attribute name="contentionCategory" type="ContentionCategory"/>
    </xs:complexType>
    <xs:complexType name="ContentionAppendix">
        <xs:sequence>
            <xs:element name="createDt" type="xs:date" minOccurs="0"/>
            <xs:element name="lastUpdateDt" type="xs:date" minOccurs="0"/>
            <xs:element name="contentionCategory" type="ContentionCategory" minOccurs="0"/>
            <xs:element name="contentionType" type="ContentionType" minOccurs="0"/>
        </xs:sequence>
        <xs:attribute name="contentionID" type="xs:string" use="required"/>
    </xs:complexType>
    <xs:complexType name="ContentionDbq">
        <xs:sequence>
            <xs:element name="createDt" type="xs:date" minOccurs="0"/>
            <xs:element name="lastUpdateDt" type="xs:date" minOccurs="0"/>
            <xs:element name="medicalOpinionInd" type="MedicalInd"/>
            <xs:element name="dbq" type="DBQ"/>
        </xs:sequence>
        <xs:attribute name="eFolderReviewInd" type="xs:boolean" use="required"/>
        <xs:attribute name="ID" type="xs:long" use="required"/>
        <xs:attribute name="contentionID" type="xs:string" use="required"/>
    </xs:complexType>
    <xs:complexType name="DbqHistorical">
        <xs:sequence>
            <xs:element name="createDt" type="xs:date" minOccurs="0"/>
            <xs:element name="expirationDt" type="xs:date" minOccurs="0"/>
            <xs:element name="lastUpdateDt" type="xs:date" minOccurs="0"/>
            <xs:element name="dbq" type="DBQ"/>
        </xs:sequence>
        <xs:attribute name="ID" type="xs:long" use="required"/>
        <xs:attribute name="contentionText" type="xs:string" use="required"/>
    </xs:complexType>
    <xs:complexType name="ContentionRatedIssue">
        <xs:sequence>
            <xs:element name="createDt" type="xs:date" minOccurs="0"/>
            <xs:element name="lastUpdateDt" type="xs:date" minOccurs="0"/>
        </xs:sequence>
        <xs:attribute name="ID" type="xs:long" use="required"/>
        <xs:attribute name="ratedIssueID" type="xs:long" use="required"/>
        <xs:attribute name="contentionID" type="xs:string" use="required"/>

        <xs:attribute name="linkStatus" type="LinkStatus"/>
        <xs:attribute name="jpaVersion" type="xs:long" use="required"/>
    </xs:complexType>
    <xs:complexType name="ContentionType">
        <xs:sequence>
            <xs:element name="createDt" type="xs:date" minOccurs="0"/>
            <xs:element name="lastUpdateDt" type="xs:date" minOccurs="0"/>
            <xs:element minOccurs="0" name="expirationDt" type="xs:date"/>
        </xs:sequence>
        <xs:attribute name="ID" type="xs:long" use="required"/>
        <xs:attribute name="code" type="xs:string" use="required"/>
        <xs:attribute name="description" type="xs:string" use="required"/>
    </xs:complexType>
    <xs:complexType name="CriminalInfo">
        <xs:sequence>
            <xs:element name="dateSentToJail" type="xs:date"/>
            <xs:element name="dateReleasedFromJail" type="xs:date"/>
            <xs:element name="paroleEndDt" type="xs:date"/>
            <xs:element name="createDt" type="xs:date" minOccurs="0"/>
            <xs:element name="lastUpdateDt" type="xs:date" minOccurs="0"/>
        </xs:sequence>
        <xs:attribute name="ID" type="xs:long" use="required"/>
        <xs:attribute name="PersonalInfo_Person_ID" type="xs:long" use="required"/>
        <xs:attribute name="crimeCommitted" type="xs:string" use="required"/>
        <xs:attribute name="verdict" type="xs:string" use="required"/>
        <xs:attribute name="sentence" type="xs:string" use="required"/>
        <xs:attribute name="stateOfCrime" type="xs:string"/>
        <xs:attribute name="countryOfCrime" type="xs:string"/>
        <xs:attribute name="onParoleInd" type="xs:boolean" use="required"/>
        <xs:attribute name="prisonName" type="xs:string"/>
        <xs:attribute name="cityOfCrime" type="xs:string" use="required"/>
    </xs:complexType>
    <xs:complexType name="DBQView">
        <xs:sequence>
            <xs:element name="createDt" type="xs:date" minOccurs="0"/>
            <xs:element name="lastUpdateDt" type="xs:date" minOccurs="0"/>
            <xs:element name="medicalOpinionInd" type="MedicalInd"/>
            <xs:element name="dbq" type="DBQ"/>
            <xs:element name="exams" type="Exam"
                        maxOccurs="unbounded" minOccurs="0"/>
        </xs:sequence>
        <xs:attribute name="suggestedInd" type="xs:boolean" use="required"/>
        <xs:attribute name="eFolderReviewInd" type="xs:boolean" use="required"/>
        <xs:attribute name="contentionID" type="xs:string" use="required"/>
    </xs:complexType>
    <xs:complexType name="Disability">
        <xs:sequence>
            <xs:element name="createDt" type="xs:date" minOccurs="0"/>
            <xs:element name="lastUpdateDt" type="xs:date" minOccurs="0"/>
            <xs:element name="expirationDt" type="xs:date" minOccurs="0"/>
        </xs:sequence>
        <xs:attribute name="ID" type="xs:long" use="required"/>
        <xs:attribute name="name" type="xs:string" use="required"/>
    </xs:complexType>
    <xs:complexType name="DisabilityBodySystem">
        <xs:sequence>
            <xs:element name="bodySystem" type="BodySystem"/>
            <xs:element name="createDt" type="xs:date" minOccurs="0"/>
            <xs:element name="lastUpdateDt" type="xs:date" minOccurs="0"/>
            <xs:element name="expirationDt" type="xs:date" minOccurs="0"/>
            <xs:element name="disability" type="Disability"/>
        </xs:sequence>
        <xs:attribute name="ID" type="xs:long" use="required"/>
    </xs:complexType>
    <xs:complexType name="Department">
        <xs:complexContent>
            <xs:extension base="LookUpValue">
                <xs:sequence>
                    <xs:element name="createDate" type="xs:date"/>
                    <xs:element name="lastUpdateDate" type="xs:date" minOccurs="0"/>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="Decoration">
        <xs:attribute name="ID" type="xs:long" use="required"/>
        <xs:attribute name="name" type="xs:string" use="required"/>
        <xs:attribute name="awardDt" type="xs:string" use="required"/>
    </xs:complexType>
    <xs:complexType name="Dependent">
        <xs:complexContent>
            <xs:extension base="Person">
                <xs:sequence>
                    <xs:element name="relationship">
                        <xs:simpleType>
                            <xs:restriction base="xs:string">
                                <xs:enumeration value="Child"/>
                                <xs:enumeration value="Parent"/>
                                <xs:enumeration value="Spouse"/>
                            </xs:restriction>
                        </xs:simpleType>
                    </xs:element>
                </xs:sequence>
                <xs:attribute name="Veteran_Person_ID" type="xs:long" use="required"/>
                <xs:attribute name="relativeStatus" type="xs:string" use="required"/>
                <xs:attribute name="veteranInd" type="xs:boolean" use="required"/>
                <xs:attribute name="relatedToVetInd" type="xs:boolean" use="required"/>
                <xs:attribute name="proofOfDependancy" type="xs:boolean" use="required"/>
                <xs:attribute name="Relationship_ID" type="xs:string"/>
                <xs:attribute name="awardInd" type="xs:boolean" use="required"/>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="DeploymentInfo">
        <xs:sequence>
            <xs:element name="inTheaterStartDt" type="xs:date"/>
            <xs:element name="inTheaterEndDt" type="xs:date"/>
            <xs:element name="createDt" type="xs:date" minOccurs="0"/>
            <xs:element name="lastUpdateDt" type="xs:date" minOccurs="0"/>
        </xs:sequence>
        <xs:attribute name="ID" type="xs:long" use="required"/>
        <xs:attribute name="ServiceInfo_ID" type="xs:long" use="required"/>
        <xs:attribute name="inTheaterDays" type="xs:int" use="required"/>
    </xs:complexType>

    <xs:complexType name="DevelopmentPlan">
        <xs:sequence>
            <xs:element name="taskList" type="DevelopmentPlanTask" maxOccurs="unbounded" minOccurs="0"/>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="DevelopmentPlanTask">
        <xs:sequence>
            <xs:element name="createDt" type="xs:date"/>
            <xs:element name="lastUpdateDt" type="xs:date"/>
            <xs:element name="vbmsUser" type="VBMSUser"/>
            <xs:element name="expirationDt" type="xs:date" minOccurs="0"/>
            <xs:element name="developmentTask" type="DevelopmentTask" minOccurs="1"/>
            <xs:element name="developmentStatus" type="DevelopmentStatus" minOccurs="1"/>
        </xs:sequence>
        <xs:attribute name="ID" type="xs:long" use="required"/>
        <xs:attribute name="claim_ID" type="xs:long" use="required"/>
        <xs:attribute name="taskTitle" type="xs:string" use="required"/>
        <xs:attribute name="obsoleteInd" type="xs:boolean" use="required"/>
    </xs:complexType>

    <xs:complexType name="DevelopmentPlanTaskActivity">
        <xs:sequence>
            <xs:element name="createDt" type="xs:date"/>
            <xs:element name="lastUpdateDt" type="xs:date"/>
            <xs:element name="vbmsUser" type="VBMSUser"/>
            <xs:element name="expirationDt" type="xs:date" minOccurs="0"/>
            <xs:element name="developmentPlanTask" type="DevelopmentPlanTask" minOccurs="1"/>
            <xs:element name="developmentActivity" type="DevelopmentActivity" minOccurs="1"/>
            <xs:element name="developmentStatus" type="DevelopmentStatus" minOccurs="0"/>
        </xs:sequence>
        <xs:attribute name="ID" type="xs:long" use="required"/>
        <xs:attribute name="letterStatus" type="xs:string"/>
    </xs:complexType>

    <xs:complexType name="DevelopmentPlanTaskActivityAction">
        <xs:sequence>
            <xs:element name="createDt" type="xs:date"/>
