    <xs:complexType name="Letter">
        <xs:sequence>
            <xs:element name="senderAddress" type="Address"
                        maxOccurs="1" minOccurs="1">
                <xs:annotation>
                    <xs:documentation>
                        The address of the sender, e.g. a regional office
                        address for a claimant letter.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="recipientAddress" type="Address"
                        maxOccurs="1" minOccurs="1">
                <xs:annotation>
                    <xs:documentation>
                        The address of the recipient. Ex. Veteran,
                        Hosiptal, POA, etc.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="generationDate" type="xs:date">
                <xs:annotation>
                    <xs:documentation>
                        Date the letter was generated.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="salutation" type="xs:string" default="Dear">
                <xs:annotation>
                    <xs:documentation>
                        Name of the person the letter is written to. Ex.
                        Veteran, Beneficiary, Doctor, etc.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="body" type="LetterBody" maxOccurs="1"
                        minOccurs="1">
                <xs:annotation>
                    <xs:documentation>
                        The content of the letter.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="signature" type="xs:string"
                        maxOccurs="1" minOccurs="1">
                <xs:annotation>
                    <xs:documentation>
                        Name of the person writing the letter. Ex. VSR
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="enclosure" type="xs:string"
                        maxOccurs="unbounded" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>
                        Used when including other documents.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="blameLine" type="xs:string"
                        maxOccurs="1" minOccurs="0">
            </xs:element>
            <xs:element name="carbonCopy" type="xs:string"
                        maxOccurs="unbounded" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>
                        Used when sending a copy of a letter to more
                        than one recipient.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="sentDate" type="xs:date">
                <xs:annotation>
                    <xs:documentation>
                        Date the letter was sent.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="stationNumber" type="RegionalOffice" minOccurs="0"/>
            <xs:element name="createDt" type="xs:date" minOccurs="0"/>
            <xs:element name="lastUpdateDt" type="xs:date" minOccurs="0"/>
            <xs:element name="vbmsUser" type="VBMSUser"/>
        </xs:sequence>
        <xs:attribute name="letterType" type="xs:string">
            <xs:annotation>
                <xs:documentation>
                    Maps to a letter type such as claimant, 3rd party,
                    follow-up, etc.
                </xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="documentType" type="xs:long" use="required"/>
        <xs:attribute name="claim_ID" type="xs:long" use="required"/>
        <xs:attribute name="unique_ID" type="xs:string"/>
        <xs:attribute name="ID" type="xs:long" use="required"/>
        <xs:attribute name="status" use="required">
            <xs:simpleType>
                <xs:restriction base="xs:string">
                    <xs:enumeration value="Pending"/>
                    <xs:enumeration value="Approved"/>
                    <xs:enumeration value="Sent"/>
                </xs:restriction>
            </xs:simpleType>
        </xs:attribute>
    </xs:complexType>
    <xs:complexType name="LetterAssociation">
